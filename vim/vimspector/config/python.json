{
  "configurations": {
    "Python - Launch": {
      "adapter": "debugpy",
      "configuration": {
        "name": "Python: Launch current file",
        "type": "python",
        "request": "launch",
        "stopOnEntry": true,
        "stopAtEntry": true,
        "console": "externalTerminal",
        "debugOptions": [],
        "cwd": "${cwd}",
        "program": "${file}"
      },
      "breakpoints": {
        "exception": {
          "raised": "N",
          "uncaught": "",
          "userUnhandled": ""
        }
      }
    }
  }
}
